<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>@ViewData["Title"]</title>
    <link rel="icon" href="favicon.png">
    <link href="/css/lumen/bootstrap.min.css" rel="stylesheet" />
    <link href="/css/site.css" rel="stylesheet" />
    <script data-main="js/main" src="js/require.js"></script>
</head>
<body>

    <nav class="navbar sticky-top navbar-expand-xl navbar-dark bg-primary">
        <div class="container justify-content-center">
            <input type="file" name="file" id="file" data-bind="event: { change: function() { send($element.files[0]) } }">
            <button id="upload-btn" class="btn btn-success btn-lg" type="submit">Upload</button>
        </div>
    </nav>
    <div class="container">
        @RenderBody()
    </div>
</body>
</html>